<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Debug</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>https://rock.citylife.church/</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>svr-acts2.citylife.local</MSDeployServiceURL>
    <DeployIisAppPath>RockTest</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>CITYLIFE\david.leigh</UserName>
    <_SavePWD>True</_SavePWD>
    <MSDeployUseChecksum>true</MSDeployUseChecksum>
  </PropertyGroup>
  <!-- copy external file and rename -->
  <Target Name="CopyTestTheme">
    <ItemGroup>
      <!-- Rename existing theme.css -->
      <_CustomFiles Include=".\Themes\Rock\Styles\theme.css" />
      <FilesForPackagingFromProject Include="%(_CustomFiles.Identity)">
        <DestinationRelativePath>.\Themes\Rock\Styles\%(RecursiveDir)theme_default.css</DestinationRelativePath>
      </FilesForPackagingFromProject>
      <!-- Add new theme.css -->
      <_CustomFiles Include=".\App_Data\PublishProfiles\theme_blue.css" />
      <FilesForPackagingFromProject Include="%(_CustomFiles.Identity)">
        <DestinationRelativePath>.\Themes\Rock\Styles\%(RecursiveDir)theme.css</DestinationRelativePath>
      </FilesForPackagingFromProject>
    </ItemGroup>
  </Target>
  <PropertyGroup>
    <CopyAllFilesToSingleFolderForMsdeployDependsOn>
      CopyTestTheme;
      $(CopyAllFilesToSingleFolderForPackageDependsOn);
    </CopyAllFilesToSingleFolderForMsdeployDependsOn>
  </PropertyGroup>
</Project>